{
  "name": "weddsite-ja",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "db:migrations": "drizzle-kit generate:sqlite --out ./app/drizzle/migrations --schema ./app/drizzle/schema.server.ts",
    "db:migrate": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "routes:gen": "routes-gen -d @routes-gen/remix"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-run/css-bundle": "2.6.0",
    "@remix-run/node": "2.6.0",
    "@remix-run/react": "2.6.0",
    "@remix-run/serve": "2.6.0",
    "@t3-oss/env-core": "0.8.0",
    "@tanstack/react-table": "8.11.8",
    "@uidotdev/usehooks": "2.4.1",
    "better-sqlite3": "9.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "0.29.3",
    "isbot": "4.1.0",
    "lucide-react": "0.330.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "2.6.0",
    "@routes-gen/remix": "0.3.7",
    "@types/better-sqlite3": "7.6.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "drizzle-kit": "0.20.14",
    "eslint": "8.38.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "routes-gen": "0.6.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
